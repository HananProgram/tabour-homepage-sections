{
  "name": "tabour/homepage-sections",
  "description": "Homepage sections package (hero, feature grid, cta) for Laravel.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/routing": "^10.0|^11.0",
    "illuminate/view": "^10.0|^11.0",
    "illuminate/filesystem": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Tabour\\Homepage\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tabour\\Homepage\\HomepageServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
